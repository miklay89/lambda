{
  "env": {
      "browser": false,
      "es2021": true
  },
  "extends": [
      "airbnb-base",
      "plugin:prettier/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint",
      "prettier"
  ],
  "rules": {
      "import/extensions": [
          "error",
          "ignorePackages",
          {
            "js": "never",
            "jsx": "never",
            "ts": "never",
            "tsx": "never"
          }
      ],
      "no-plusplus": "off",
      "func-names": "off",
      "no-console": "off",
      "no-shadow": "off",
      "no-redeclare": "off",
      "no-extend-native": "off",
      "@typescript-eslint/no-shadow": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
  }

}